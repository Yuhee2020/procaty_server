{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "buildCommand": "next build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@now/node"

    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/index.js"
    }
  ]
}